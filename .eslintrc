{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "arrow-parens": "off",
        "indent": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "operator-linebreak": "off"
    },
   "extends": ["eslint:recommended", "google"]
}